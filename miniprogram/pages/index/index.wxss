/**index.wxss**/
page {
  background: url('https://img.jogiter.cn/third/shares.png');
  background-size: 100%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

swiper {
  width: 100%;

  padding-bottom: 10rpx;


}

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
}

.buy {
  width: 223rpx;
  height: 90rpx;
  position: fixed;
  bottom: 30rpx;
  padding: 0;
  background: none;
}

.buy::after {
  border: none;
}

.buy1 {
  width: 223rpx;
  height: 90rpx;
}

.hint {
  font-size: 28rpx;
  color: white;
  margin-bottom: 30rpx;
  background: none;
}

.hint::after {
  border: none;
}

.back {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 90%;
  background-color: #f5d4a2;
  border: 4px solid #ee9c54;
  border-radius: 10rpx;
  /* padding-bottom: 30rpx; */
  margin-bottom: 10rpx;
}

.bottom {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 120rpx;
  background: linear-gradient(rgba(255, 255, 255, 0), rgba(234, 122, 43, 1));
}

.back1 {
  padding-top: 40rpx;
  padding-bottom: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 90%;
  z-index: 1001;
  position: fixed;
  top: 100rpx;
  background-color: #dca95b;
  border: 4px solid #6a3906;
  border-radius: 10rpx;
}

.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.8;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}

.hold {
  width: 100%;
  height: 650rpx;
  overflow: scroll;
}

.tab {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.meiri {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 50%;
  height: 87rpx;
  text-align: center;
  margin-bottom: 20rpx;
  background-color: #ee9c54;
  color: white;
}

.focus {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 50%;
  height: 87rpx;
  text-align: center;
  margin-bottom: 20rpx;
  background-color: #f5d4a2;
  color: #fe5b2f;
  font-size: 36rpx;
  font-weight: bold;
  font-family: STHupo;
}

.focus.no {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 50%;
  height: 87rpx;
  text-align: center;
  margin-bottom: 20rpx;
  background-color: #ee9c54;
  color: white;
  font-size: 32rpx;
  font-weight: normal;
}

.gushen{
  margin-left: 10rpx;
  width:80rpx;
  background-color: #eb6100;
  color:white;
  font-size: 28rpx;
  text-align: center;
  border-radius: 5rpx;
}
.zichan{
  width:40%;

  color:#f88116;
  font-size: 44rpx;
  text-align: right;
  padding-right: 20rpx;
  font-weight: bold;


}
.left{

  width:60%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;


}
.row {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
}

.row1 {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  width: 100%;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
}

.input {
  width: 20%;
  background: white;
  border-radius: 8rpx;
  padding-left: 10rpx;
  margin-left: 10rpx;
}

.sure {
  width: 258rpx;
  height: 90rpx;
}

.sure1 {
  width: 258rpx;
  height: 90rpx;
  padding: 0;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.company {
  width: 60%;
  color: #6a3906;
  font-size: 32rpx;
  margin-left: 40rpx;
}

.company1 {
  color: #6a3906;
  font-size: 32rpx;
  width: 40%;
  margin-left: 40rpx;
}

.company2 {
  width: 60%;
  color: #6a3906;
  font-size: 32rpx;
  margin-left: 40rpx;
  margin-right: 60rpx;
}

.price {
  width: 40%;
  color: #6a3906;
  font-size: 32rpx;
  text-align: center;
}

.rateminus {
  color: #005e15;
  font-size: 28rpx;
  width: 20%;
  margin-left: 10rpx;
  margin-right: 20rpx;
}

.rate {
  color: #a40000;
  font-size: 28rpx;
  width: 20%;
  margin-left: 10rpx;
  margin-right: 20rpx;
}

.nickkname {
  width: 180rpx;
  margin-left: 20rpx;
  font-size: 32rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.nickname {
  width:auto;
  max-width: 180rpx;

  margin-left: 20rpx;
  font-size: 32rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.total {
  margin: 18rpx;
  width: 50%;
  text-align: right;
  color: #6a3906;
  font-weight: bold;
  font-size: 32rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
}

.totalbutton {
  width:auto;
  padding: 0rpx;
  margin: 0rpx;
  background: none;
  color: #6a3906;
  font-weight: bold;
  font-size: 36rpx;
}

.totalbutton::after {
  border: none;
}

.userinfo, .uploader, .tunnel {
  margin-top: 40rpx;
  height: 140rpx;
  width: 100%;
  background: #fff;
  border: 1px solid rgba(0, 0, 0, 0.1);
  border-left: none;
  border-right: none;
  display: flex;
  flex-direction: row;
  align-items: center;
  transition: all 300ms ease;
}

.asset {
  width: 671rpx;
  height: 182rpx;

  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;

  margin-top: 130rpx;
  margin-bottom: 20rpx;
  background-image:url(data:image/png;base64,);
  background-size: 100%;

}


.user {
  display: flex;
  flex-flow: row;
  justify-content: flex-start;
  align-items: center;
}

.useravatar {
  width: 80rpx;
  height: 80rpx;
  margin-left: 30rpx;
  border-radius: 50%;
  background-repeat: round;
  background-size: contain;
  background-color: white;
}

.useravatar:after {
  border: none;
}

.userinfo-nickname::after {
  border: none;
}

.uploader, .tunnel {
  height: auto;
  padding: 0 0 0 40rpx;
  flex-direction: column;
  align-items: flex-start;
  box-sizing: border-box;
}

.uploader-text, .tunnel-text {
  width: 100%;
  line-height: 52px;
  font-size: 34rpx;
  color: #007aff;
}

.uploader-container {
  width: 100%;
  height: 400rpx;
  padding: 20rpx 20rpx 20rpx 0;
  display: flex;
  align-content: center;
  justify-content: center;
  box-sizing: border-box;
  border-top: 1px solid rgba(0, 0, 0, 0.1);
}

.uploader-image {
  width: 100%;
  height: 360rpx;
}

.tunnel {
  padding: 0 0 0 40rpx;
}

.tunnel-text {
  position: relative;
  color: #222;
  display: flex;
  flex-direction: row;
  align-content: center;
  justify-content: space-between;
  box-sizing: border-box;
  border-top: 1px solid rgba(0, 0, 0, 0.1);
}

.tunnel-text:first-child {
  border-top: none;
}

.tunnel-switch {
  position: absolute;
  right: 20rpx;
  top: -2rpx;
}

.disable {
  color: #888;
}

.service {
  position: fixed;
  right: 40rpx;
  bottom: 40rpx;
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  background: linear-gradient(#007aff, #0063ce);
  box-shadow: 0 5px 10px rgba(0, 0, 0, 0.3);
  display: flex;
  align-content: center;
  justify-content: center;
  transition: all 300ms ease;
}

.service-button {
  position: absolute;
  top: 40rpx;
}

.service:active {
  box-shadow: none;
}

.request-text {
  padding: 20rpx 0;
  font-size: 24rpx;
  line-height: 36rpx;
  word-break: break-all;
}
